      android:allowBackup="false"
      android:installLocation="auto" 
      android:usesCleartextTraffic="true"
      android:theme="@style/AppTheme">
     
    <activity android:name=".MainActivity" android:label="@string/app_name" android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:launchMode="singleTask" android:windowSoftInputMode="adjustResize" android:screenOrientation="portrait" android:hardwareAccelerated="true" android:exported="true">
